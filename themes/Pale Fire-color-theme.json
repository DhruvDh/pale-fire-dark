{
	"name": "Pale Fire",
	"type": "dark",
	"colors": {
		"editor.background": "#3F3F3F",
		"editor.selectionBackground": "#2B2B2B",
		"editor.foreground": "#DCDCCC",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"foreground": "#DCDCCC",
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage",
				"keyword.operator.new",
				"keyword.operator.expression",
				"variable.language.this",
				"constant.language.null",
			],
			"settings": {
				"foreground": "#F0DFAF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Punctuation",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#DCDCCC",
				"fontStyle": ""
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#7F9F7F",
			}
		},
		{
			"name": "Function",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#93E0E3",
			}
		},
		{
			"name": "Type",
			"scope": [
				"entity.name.type",
				"storage.type.core.rust"
			],
			"settings": {
				"foreground": "#7CB8BB",
				"fontStyle": "",
			}
		},
		{
			"name": "Primitive",
			"scope": [
				"support.type.primitive",
				"support.type.builtin"
			],
			"settings": {
				"foreground": "#8CD0D3",
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#CC9393",
			}
		},
		{
			"name": "Constant",
			"scope": "constant",
			"settings": {
				"foreground": "#BFEBBF",
			}
		}
	]
}
